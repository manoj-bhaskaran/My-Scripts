# Ensure XML file is stored in UTF-8
"Windows Task Scheduler/PostgreSQL job_scheduler Backup.xml" working-tree-encoding=UTF-8
*.ps1 text eol=crlf
*.psd1 text eol=crlf
*.sql filter=lfs diff=lfs merge=lfs -text
*.dump filter=lfs diff=lfs merge=lfs -text
*.mp4 filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
